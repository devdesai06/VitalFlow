CREATE DATABASE BLOOD_DONATION;
USE blood_donation;
-- =====================================================
-- DATABASE: BLOOD BANK MANAGEMENT SYSTEM
-- =====================================================
SET SQL_SAFE_UPDATES = 0;
-- drop database blood_donation 
-- =====================================================
-- 1. DONOR TABLE
-- =====================================================
CREATE TABLE Donor (
    Donor_ID INT AUTO_INCREMENT PRIMARY KEY,
    Name VARCHAR(100) NOT NULL,
    Gender VARCHAR(10) CHECK (Gender IN ('Male', 'Female', 'Other')),
    DOB DATE,
    BloodGroup VARCHAR(5) NOT NULL,
    Contact_No VARCHAR(15) UNIQUE,
    Email VARCHAR(100) UNIQUE,
    Address VARCHAR(255),
    Registration_Date DATE
);


-- =====================================================
-- 2. BLOOD TYPE TABLE
-- =====================================================
CREATE TABLE Blood_Type (
    BloodType_ID INT AUTO_INCREMENT PRIMARY KEY,
    BloodGroup VARCHAR(5) NOT NULL UNIQUE,
    RH_Factor CHAR(1) CHECK (RH_Factor IN ('+', '-')),
    Description VARCHAR(100)
);

-- =====================================================
-- 3. BLOOD TEST TABLE
-- =====================================================
CREATE TABLE Blood_Test (
    Test_ID INT AUTO_INCREMENT PRIMARY KEY,
    HIV_Test VARCHAR(10) CHECK (HIV_Test IN ('Positive', 'Negative')),
    Hepatitis VARCHAR(10) CHECK (Hepatitis IN ('Positive', 'Negative')),
    Malaria VARCHAR(10) CHECK (Malaria IN ('Positive', 'Negative')),
    Test_Date DATE,
    Result VARCHAR(20) CHECK (Result IN ('Safe', 'Unsafe'))
);

-- =====================================================
-- 4. BLOOD DONATION TABLE
-- =====================================================

CREATE TABLE IF NOT EXISTS Blood_Donation (
    Donation_ID INT AUTO_INCREMENT PRIMARY KEY,
    Donor_ID INT,
    BloodType_ID INT,
    Test_ID INT,
    Donation_Date DATE NOT NULL,
    Quantity_ml INT CHECK (Quantity_ml > 0),
    Status VARCHAR(50) CHECK (Status IN ('Stored', 'Used', 'Discarded')),
    FOREIGN KEY (Donor_ID) REFERENCES Donor(Donor_ID),
    FOREIGN KEY (BloodType_ID) REFERENCES Blood_Type(BloodType_ID),
    FOREIGN KEY (Test_ID) REFERENCES Blood_Test(Test_ID)
);


-- =====================================================
-- 5. BLOOD INVENTORY TABLE
-- =====================================================
CREATE TABLE Blood_Inventory (
    Inventory_ID INT AUTO_INCREMENT PRIMARY KEY,
    BloodType_ID INT,
    Quantity_Units INT CHECK (Quantity_Units >= 0),
    Last_Updated DATE,
    FOREIGN KEY (BloodType_ID) REFERENCES Blood_Type(BloodType_ID)
);

-- =====================================================
-- 6. HOSPITAL TABLE
-- =====================================================
CREATE TABLE Hospital (
    Hospital_ID INT AUTO_INCREMENT PRIMARY KEY,
    Hospital_Name VARCHAR(100) NOT NULL UNIQUE,
    Contact_No VARCHAR(15),
    Email VARCHAR(100),
    Address VARCHAR(255)
);

-- =====================================================
-- 7. BLOOD REQUEST TABLE
-- =====================================================
CREATE TABLE Blood_Request (
    Request_ID INT AUTO_INCREMENT PRIMARY KEY,
    Hospital_ID INT,
    BloodType_ID INT,
    Requested_Units INT CHECK (Requested_Units > 0),
    Request_Date DATE,
    Status VARCHAR(20) CHECK (Status IN ('Pending', 'Approved', 'Completed')),
    FOREIGN KEY (Hospital_ID) REFERENCES Hospital(Hospital_ID),
    FOREIGN KEY (BloodType_ID) REFERENCES Blood_Type(BloodType_ID)
);

-- =====================================================
-- 8. BLOOD DISTRIBUTION TABLE
-- =====================================================
CREATE TABLE Blood_Distribution (
    Distribution_ID INT AUTO_INCREMENT PRIMARY KEY,
    Request_ID INT,
    Inventory_ID INT,
    Quantity_Distributed INT CHECK (Quantity_Distributed > 0),
    Dispatch_Date DATE,
    Delivered_To VARCHAR(100),
    Status VARCHAR(20) CHECK (Status IN ('Delivered', 'Pending', 'Cancelled')),
    FOREIGN KEY (Request_ID) REFERENCES Blood_Request(Request_ID),
    FOREIGN KEY (Inventory_ID) REFERENCES Blood_Inventory(Inventory_ID)
);

-- =====================================================
-- 9. STAFF TABLE
-- =====================================================
CREATE TABLE Staff (
    Staff_ID INT AUTO_INCREMENT PRIMARY KEY,
    Name VARCHAR(100) NOT NULL,
    Role VARCHAR(50),
    Contact_No VARCHAR(15) UNIQUE,
    Email VARCHAR(100),
    Join_Date DATE,
    Shift VARCHAR(20) CHECK (Shift IN ('Morning', 'Evening', 'Night'))
);

-- =====================================================
-- 10. CAMPAIGN TABLE
-- =====================================================
CREATE TABLE Campaign (
    Campaign_ID INT AUTO_INCREMENT PRIMARY KEY,
    Campaign_Name VARCHAR(100) NOT NULL,
    Location VARCHAR(100),
    Date DATE,
    Organized_By VARCHAR(100),
    Collected_Units INT CHECK (Collected_Units >= 0)
);
