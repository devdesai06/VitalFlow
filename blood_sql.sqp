-- ======================================================================
-- UNIQUE FEATURES
-- ======================================================================
USE blood_donation;
-- ======================================================================
-- 1.Monthly Donation Summary Report
-- Feature: Generate reports showing total donations and units per month.
-- ======================================================================
CREATE VIEW Monthly_Donation_Report AS
SELECT 
    DATE_FORMAT(Donation_Date, '%Y-%m') AS Month,
    COUNT(Donation_ID) AS Total_Donations,
    SUM(Quantity_ml) AS Total_Quantity_ml
FROM Blood_Donation
GROUP BY DATE_FORMAT(Donation_Date, '%Y-%m')
ORDER BY Month DESC;
SELECT * FROM Monthly_Donation_Report;


-- ==================================================================================
-- 2.Campaign Performance Tracker
-- Feature: Track how much each campaign contributed to the total blood collected.
-- ==================================================================================

CREATE VIEW Campaign_Performance AS
SELECT 
    Campaign_Name,
    Location,
    Organized_By,
    Date AS Campaign_Date,
    Collected_Units,
    CASE
        WHEN Collected_Units >= 80 THEN 'Excellent'
        WHEN Collected_Units BETWEEN 60 AND 79 THEN 'Good'
        WHEN Collected_Units BETWEEN 40 AND 59 THEN 'Average'
        ELSE 'Low'
    END AS Performance_Rating
FROM Campaign
ORDER BY Collected_Units asc;
SELECT * FROM Campaign_Performance;

-- ================================================================
-- 3. Safe Blood Availability Report
-- Feature: Identify all safe, stored blood units by blood type
-- ================================================================
CREATE VIEW Safe_Blood_Availability AS
SELECT 
    bd.Donation_ID,
    d.Name AS Donor_Name,
    bt.BloodGroup,
    bd.Quantity_ml,
    bd.Status,
    bt.RH_Factor,
    bd.Donation_Date
FROM Blood_Donation bd
JOIN Donor d ON bd.Donor_ID = d.Donor_ID
JOIN Blood_Type bt ON bd.BloodType_ID = bt.BloodType_ID
JOIN Blood_Test t ON bd.Test_ID = t.Test_ID
WHERE t.Result = 'Safe' AND bd.Status = 'Stored'
ORDER BY bd.Donation_Date DESC
LIMIT 10;

SELECT * FROM Safe_Blood_Availability;


-- ================================================================
-- 4. Blood Type Stock Overview
-- Feature: Display inventory levels and alert for low stock
-- ================================================================
CREATE VIEW Blood_Stock_Overview AS
SELECT 
    bt.BloodGroup,
    bt.RH_Factor,
    bi.Quantity_Units,
    CASE
        WHEN bi.Quantity_Units < 10 THEN 'Critical Low'
        WHEN bi.Quantity_Units BETWEEN 10 AND 20 THEN 'Low'
        ELSE 'Sufficient'
    END AS Stock_Status,
    bi.Last_Updated
FROM Blood_Inventory bi
JOIN Blood_Type bt ON bi.BloodType_ID = bt.BloodType_ID
ORDER BY bi.Quantity_Units ASC
LIMIT 10;

SELECT * FROM Blood_Stock_Overview;



-- ================================================================
-- 5. Hospital Request Summary
-- Feature: Track total, approved, and completed requests per hospital
-- ================================================================
CREATE VIEW Hospital_Request_Summary AS
SELECT 
    h.Hospital_Name,
    COUNT(r.Request_ID) AS Total_Requests,
    SUM(CASE WHEN r.Status = 'Approved' THEN 1 ELSE 0 END) AS Approved_Requests,
    SUM(CASE WHEN r.Status = 'Completed' THEN 1 ELSE 0 END) AS Completed_Requests,
    SUM(r.Requested_Units) AS Total_Units_Requested
FROM Hospital h
JOIN Blood_Request r ON h.Hospital_ID = r.Hospital_ID
GROUP BY h.Hospital_Name
ORDER BY Total_Requests DESC
LIMIT 10;

SELECT * FROM Hospital_Request_Summary;



-- ================================================================
-- 6. Staff Shift Summary by Role
-- Feature: Show how many staff members work in each role and shift
-- ================================================================
CREATE VIEW Staff_Shift_Summary AS
SELECT 
    Role,
    Shift,
    COUNT(*) AS Total_Staff
FROM Staff
GROUP BY Role, Shift
ORDER BY Role, Shift;

SELECT * FROM Staff_Shift_Summary;
